{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "clean",
      "type": "process",
      "command": "dotnet",
      "args": [
        "clean",
        "${workspaceFolder}/Backend/MoviePerspectives.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/Backend/MoviePerspectives.csproj",
        "--configuration",
        "Release"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
